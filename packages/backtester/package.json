{
  "name": "@gicm/backtester",
  "version": "0.1.0",
  "description": "Trading backtester - strategy testing, risk analytics, Monte Carlo simulation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "gicm-backtest": "./dist/cli.js"
