    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gicm/agent-core": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "keywords": ["backtester", "trading", "strategy", "analytics", "monte-carlo", "web3"],
  "license": "MIT"
}
