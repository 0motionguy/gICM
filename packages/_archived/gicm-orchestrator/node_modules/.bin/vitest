#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/vitest@2.1.9_@types+node@20_4d9aaa4b141c7e41bd64c2e14906ed93/node_modules/vitest/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/vitest@2.1.9_@types+node@20_4d9aaa4b141c7e41bd64c2e14906ed93/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/vitest@2.1.9_@types+node@20_4d9aaa4b141c7e41bd64c2e14906ed93/node_modules/vitest/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/vitest@2.1.9_@types+node@20_4d9aaa4b141c7e41bd64c2e14906ed93/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
