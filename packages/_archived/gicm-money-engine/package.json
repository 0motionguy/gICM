{
  "name": "@gicm/money-engine",
  "version": "1.0.0",
  "description": "Self-funding engine for gICM platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gicm-money": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts --clean",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "trade": "node dist/cli.js trade",
    "status": "node dist/cli.js status",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.0",
    "decimal.js": "^10.4.0",
    "cron": "^3.1.0",
    "eventemitter3": "^5.0.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "zod": "^3.23.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "tsup": "^8.0.0",
    "tsx": "^4.10.0",
    "@types/node": "^20.0.0"
  },
  "keywords": ["gicm", "treasury", "trading", "solana", "defi"],
  "author": "gICM",
  "license": "MIT"
}
