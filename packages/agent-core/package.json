{
  "name": "@gicm/agent-core",
  "version": "1.0.0",
  "description": "Core utilities for gICM Web3 AI agents - shared types, base classes, and chain abstractions",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./chains": {
      "import": "./dist/chains/index.js",
      "types": "./dist/chains/index.d.ts"
    },
    "./llm": {
      "import": "./dist/llm/index.js",
      "types": "./dist/llm/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "web3",
    "ai-agents",
    "solana",
    "evm",
    "langchain",
    "gicm"
  ],
  "author": "gICM",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
