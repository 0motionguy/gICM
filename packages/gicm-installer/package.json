{
  "name": "@gicm/installer",
  "version": "1.0.0",
  "description": "Install gICM skill pack into OpenClaw — copies SKILL.md files to ~/.openclaw/skills/",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gicm-install": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "openclaw",
    "gicm",
    "skill-installer",
    "ai-agents"
  ],
  "author": "Mirko Basil Dölger <mirko@gicm.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0motionguy/gicm-installer.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
