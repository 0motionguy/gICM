{
  "name": "@gicm/gicm-orchestrator",
  "version": "0.1.0",
  "description": "The Brain - Autonomous orchestrator for gICM that discovers, evaluates, and integrates valuable tech",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gicm-orchestrator": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js start",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gicm/activity-logger": "workspace:*",
    "@gicm/agent-core": "workspace:*",
    "@gicm/builder-agent": "workspace:*",
    "@gicm/decision-agent": "workspace:*",
    "@gicm/deployer-agent": "workspace:*",
    "@gicm/hunter-agent": "workspace:*",
    "@gicm/refactor-agent": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cron": "^3.1.0",
    "ora": "^7.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cron": "^2.4.0",
    "@types/node": "^20.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.0"
  },
  "files": ["dist"],
  "keywords": ["gicm", "orchestrator", "autonomous", "ai-agents"],
  "license": "MIT"
}
